<% @ad.comments.each do |comment| %>
  <p><%= comment.body %></p>
  <small class="text-muted"><%= comment.member.email %> <%= comment.created_at %></small>
  <br>
  <% if comment.member.id == current_member.id %>
  	<%= rating_for_user @ad, current_member, 'quality', star: 10 %>
  <% end %>
  <hr>
<% end %>
