<div class="col-lg-12">
  <br>
  <div class="card-header">
    <%= form_tag site_search_path, method: :get, class: "form-inline my-2 my-lg-0" do %>
      <%= text_field_tag(:q, params[:q], {class: "form-control mr-sm-3", placeholder: "Search", "aria-label": "Search", style: "width: 91% !important;"}) %>
      <%= submit_tag("Search", class: "btn btn-outline-success my-2 my-sm-0") %>
    <% end %>
  </div>
  <br>
</div>
